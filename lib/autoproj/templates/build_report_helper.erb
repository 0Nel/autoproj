<% @first_line = true -%>
{
   "build_report" : {
       "timestamp": "<%= Time.now %>",
       "packages": [<% package_list.each do |pkg_name| -%><% if @first_line == false  %>,<% end -%><% @first_line = false  %>
           {
               "name": "<%= Autobuild::Package[pkg_name].name %>",
               "import_invoked": "<%= Autobuild::Package[pkg_name].import_invoked? %>",
               "prepare_invoked": "<%= Autobuild::Package[pkg_name].prepare_invoked? %>",
               "build_invoked": "<%= Autobuild::Package[pkg_name].build_invoked? %>",
               "failed": "<%= Autobuild::Package[pkg_name].failed? %>",
               "imported": "<%= Autobuild::Package[pkg_name].imported? %>",
               "prepared": "<%= Autobuild::Package[pkg_name].prepared? %>",
               "built": "<%= Autobuild::Package[pkg_name].built? %>"
           }<% end -%>
       ]
   }
}
